{
    "manifest_version": 2,
    "name": "Hello Extension",
    "version": "1.0",
    "description": "A basic Chrome extension",
    "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
    },
    "permissions": [
      "activeTab"
    ],
    "icons": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  }
  