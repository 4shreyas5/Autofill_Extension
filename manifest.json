{
    "manifest_version": 2,
    "name": "Autofill",
    "version": "1.0.0",
    "description": "A basic Chrome extension to Autofill your information to make your form filling easier",
    "manifest_version": 2,
    "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
    },
    "permissions": [
      "activeTab", "storage"
    ],
    "icons": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  }
  