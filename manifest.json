{
  "manifest_version": 2,
  "name": "AutoFill Extension",
  "version": "1.0",
  "description": "A simple Chrome extension for storing and auto-filling user details.",
  "browser_action": {
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    },
    "default_popup": "popup.html"
  },
  "permissions": ["storage"]
}